   "source": [
    "import numpy as np"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 157,
   "outputs": [],
   "source": [
    "def smth(x):\n",
    "    index = np.arange(0, x.shape[0])\n",
    "    index = index[x != 0]\n",
    "    y = np.zeros(x.shape[0])\n",
    "    y[index[1::]] = x[index[:-1]]\n",
    "    return np.cumsum(x) - np.cumsum(y)"
   ],
   "metadata": {
    "collapsed": false
   }
  },
  {
   "cell_type": "code",
   "execution_count": 159,
   "outputs": [
