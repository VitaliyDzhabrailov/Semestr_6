   "source": [
    "def smth(x, y):\n",
    "\n",
    "    z_1 = (x * y != 0)\n",
    "\n",
    "    z_2 = ((abs(x) + abs(y)) != 0)\n",
    "\n",
    "    return z_1.sum(), z_2.sum()"
   ],
   "metadata": {
    "collapsed": false
   }
  },
